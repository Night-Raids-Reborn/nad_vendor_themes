<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="#555555">
    <item android:id="@android:id/mask">
        <shape>
            <solid android:color="@*android:color/notification_material_background_color" />
            <corners android:radius="100dp" />
        </shape>
    </item>
    <item android:id="@android:id/background">
        <shape android:shape="oval">
            <solid android:color="@*android:color/notification_material_background_color" />
        </shape>
    </item>
    <item android:left="3.0dip" android:top="3.0dip" android:right="3.0dip" android:bottom="3.0dip" >
	    <shape android:tint="?android:attr/textColorPrimary" android:shape="oval">
		    <solid android:color="#14ffffff" />
	    </shape>
    </item>
</ripple>

